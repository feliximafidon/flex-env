{
    "name": "feliximafidon/flex-env",
    "description": "Edit your .env file in Laravel directly from the command line. Fork of sven/flexenv",
    "keywords": ["flexible", "env", "laravel", "artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Imafidon",
            "email": "hi@feliximafidon.com",
            "homepage": "https://feliximafidon.com"
        },
        {
            "name": "Sven Luijten (code credits)",
            "email": "svenluijten1995@gmail.com",
            "homepage": "https://svenluijten.com"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "illuminate/support": "^5.0",
        "laravel/framework": "^5.5|^6.20.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0",
        "graham-campbell/testbench": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "feliximafidon\\FlexEnv\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "feliximafidon\\FlexEnv\\FlexEnvServiceProvider"
            ]
        }
    }
}
